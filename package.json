{
  "name": "prettier-plugin-tailwindcss-extra",
  "version": "0.1.0",
  "description": "Adds Tailwind class sorting to a set of languages that are not yet supported by prettier",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsup src/main.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "tsc --noEmit",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check ."
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13"
  }
}
